<%= form_with(model: try, local: true) do |form| %>
  <% if try.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(try.errors.count, "error") %> prohibited this try from being saved:</h2>

      <ul>
      <% try.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :try_name %>
  </div>

  <div class="field">
    <%= form.label :score %>
    <%= form.text_field :score, id: :try_score %>
  </div>

  <div class="field">
    <%= form.label :frame_id %>
    <%= form.text_field :frame_id, id: :try_frame_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
